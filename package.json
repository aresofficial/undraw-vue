{
    "name": "undraw-vue",
    "description": "Undraw illustration library for Vue 3",
    "private": false,
    "type": "module",
    "version": "0.0.10",
    "files": [
        "dist",
        "src/components/"
    ],
    "main": "./dist/undraw-vue.umd.cjs",
    "module": "./dist/undraw-vue.js",
    "exports": {
        ".": {
            "import": "./dist/undraw-vue.js",
            "require": "./dist/undraw-vue.umd.cjs"
        }
    },
    "types": "./dist/index.d.ts",
    "scripts": {
        "dev": "vite",
        "build": "vue-tsc && vite build",
        "preview": "vite preview"
    },
    "keywords": [
        "vue",
        "vue3",
        "undraw",
        "illustration"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/aresofficial/undraw-vue.git"
    },
    "author": "Volodymyr Rets",
    "peerDependencies": {
        "vue": "^3.0.0"
    },
    "devDependencies": {
        "@vitejs/plugin-vue": "^5.0.0",
        "typescript": "^5.0.0",
        "vite": "^5.0.0",
        "vite-plugin-dts": "^3.0.0",
        "vue": "^3.0.0",
        "vue-tsc": "^2.0.21"
    }
}